    "puppeteer": "22.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "satori": "0.10.13"
  },
  "devDependencies": {
    "@types/eslint": "8.56.2",
    "@types/eslint-config-prettier": "6.11.3",
